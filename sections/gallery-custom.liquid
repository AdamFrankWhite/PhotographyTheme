<style>
img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}
.parent {
    width: 80%;
    margin: auto;
    display: grid;
    grid-template-columns: repeat(9, 1fr);
    grid-template-rows: repeat(12, 1fr);
    gap: 8px;
}
    
.image1 {
    grid-column: span 3 / span 3;
    grid-row: span 5 / span 5;
}

.image2 {
    grid-column: span 2 / span 2;
    grid-row: span 2 / span 2;
    grid-column-start: 4;
}

.image3 {
    grid-column: span 2 / span 2;
    grid-row: span 2 / span 2;
    grid-column-start: 4;
    grid-row-start: 3;
}

.image4 {
    grid-column: span 2 / span 2;
    grid-row: span 4 / span 4;
    grid-column-start: 4;
    grid-row-start: 5;
}

.image5 {
    grid-column: span 4 / span 4;
    grid-row: span 5 / span 5;
    grid-column-start: 6;
    grid-row-start: 1;
}

.image6 {
    grid-column-start: 6;
    grid-row-start: 6;
}

.image7 {
    grid-column-start: 7;
    grid-row-start: 6;
}

.image8 {
    grid-column-start: 8;
    grid-row-start: 6;
}

.image9 {
    grid-column-start: 9;
    grid-row-start: 6;
}

.image10 {
    grid-column: span 3 / span 3;
    grid-row: span 3 / span 3;
    grid-column-start: 1;
    grid-row-start: 6;
}

.image11 {
    grid-column: span 3 / span 3;
    grid-row: span 3 / span 3;
    grid-column-start: 1;
    grid-row-start: 9;
}

.image12 {
    grid-column-start: 1;
    grid-row-start: 12;
}

.image13 {
    grid-column-start: 2;
    grid-row-start: 12;
}

.image14 {
    grid-column-start: 3;
    grid-row-start: 12;
}

.image15 {
    grid-column: span 4 / span 4;
    grid-row: span 4 / span 4;
    grid-column-start: 4;
    grid-row-start: 9;
}

.image16 {
    grid-column: span 2 / span 2;
    grid-row: span 2 / span 2;
    grid-column-start: 6;
    grid-row-start: 7;
}

.image17 {
    grid-column: span 2 / span 2;
    grid-row: span 4 / span 4;
    grid-column-start: 8;
    grid-row-start: 7;
}

.image18 {
    grid-column: span 2 / span 2;
    grid-row: span 2 / span 2;
    grid-column-start: 8;
    grid-row-start: 11;
}
        

</style>

